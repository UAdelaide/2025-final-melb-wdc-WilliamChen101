<!DOCTYPE html>
<html>
<head>
  <title>User List</title>
</head>
<body>
  <h1>User List</h1>
  <table border="1">
    <tr>
      <th>ID</th><th>Username</th><th>Email</th><th>Role</th>
    </tr>
    <% users.forEach(user => { %>
      <tr>
        <td><%= user.user_id %></td>
        <td><%= user.username %></td>
        <td><%= user.email %></td>
        <td><%= user.role %></td>
      </tr>
    <% }) %>
  </table>
</body>
</html>